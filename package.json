{
  "type": "module",
  "name": "next-prisma-docker",
  "version": "0.1.0",
  "private": true,
  "description": "iFood with Next.js + Prisma",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma:migrate && next dev -p 3002",
    "build": "next build",
    "start:dev": "yarn prisma:migrate && next start",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "dkc:dev:up": "docker-compose -f docker-compose.yml up",
    "dkc:dev:build": "docker-compose -f docker-compose.yml up --build",
    "dkc:prod:up": "docker-compose -f docker-compose.prod.yml up",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@tailwindcss/forms": "^0.5.2",
    "daisyui": "^2.24.0",
    "framer-motion": "^7.1.0",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-scroll-to-top": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@tailwindcss/typography": "^0.5.4",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "prisma": "^4.2.1",
    "tailwindcss": "^3.1.7",
    "typescript": "4.7.4"
  }
}
